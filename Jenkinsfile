pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'echo "BUILDING..."'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "TESTING..."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "DEPLOYING..."'
            }
        }
    }
}